{
    "name": "jsq/psr6-encrypting-decorator",
    "description": "For when policy or paranoia prevent you from leaving sensitive cache entries unencrypted at rest.",
    "minimum-stability": "stable",
    "license": "Apache-2.0",
    "keywords": ["psr", "psr-6", "cache"],
    "authors": [
        {
            "name": "Jonathan Eskew",
            "email": "jonathan@jeskew.net"
        }
    ],
    "require": {
        "php": "^5.5|^7.0",
        "ext-openssl": "*",
        "psr/cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "cache/integration-tests": "^0.2.0",
        "cache/doctrine-adapter": "^0.3.0",
        "doctrine/cache": "^1.6",
        "paragonie/random_compat": "^2.0"
    },
    "suggest": {
        "paragonie/random_compat": "To use with PHP <7.0"
    },
    "autoload": {
        "psr-4": {
            "Jsq\\CacheEncryption\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jsq\\CacheEncryption\\": "tests/"
        }
    }
}
